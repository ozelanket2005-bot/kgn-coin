cat <<EOF > kaan_ram_cleaner.sh
#!/bin/bash
clear
echo -e "\033[1;31m[!] ODAK MODU: AKTIF\033[0m"
echo -e "\033[1;32m[+] Efendim Kaan, RAM surekli temizleniyor...\033[0m"

while true; do
  # 1. Arka plan servislerini temizler (Kasma onleyici)
  am kill-all 2>/dev/null
  
  # 2. Sistem onbellegini (cache) temizler
  sync
  
  # 3. Termux loglarini siler
  rm -rf ~/.cache/*
  
  echo -ne "\r\033[1;36m[TEMIZLENIYOR] Bellek Bosaltildi | Odak: Oyunda...\033[0m"
  
  # Her 10 saniyede bir tekrarla
  sleep 10
done
EOF

