import os, requests
from gtts import gTTS

def elma_konus(metin):
    print("Elma: " + metin)
    tts = gTTS(text=metin, lang='tr')
    tts.save("elma_ses.mp3")
    os.system("mpv elma_ses.mp3 > /dev/null 2>&1")

def arastir(konu):
    elma_konus(f"Hemen bakıyorum Efendim Kaan. {konu} hakkında bilgi topluyorum.")
    try:
        res = requests.get(f"https://tr.wikipedia.org/api/rest_v1/page/summary/{konu}")
        data = res.json()
        if 'extract' in data:
            elma_konus(data['extract'][:300] + "...")
        else:
            elma_konus("Maalesef bu konuda detaylı bilgi bulamadım.")
    except:
        elma_konus("İnternet bağlantımda bir sorun var Efendim.")

def elma_baslat():
    elma_konus("Merhaba Efendim Kaan. Siri modum aktif. Sizi dinliyorum.")
    while True:
        komut = input("\nKomutunuz: ").lower()
        if "araştır" in komut:
            konu = komut.replace("araştır", "").strip()
            arastir(konu)
        elif "nasılsın" in komut:
            elma_konus("Sizinle beraber geliştiğim için harikayım Efendim.")
        elif "kapat" in komut:
            elma_konus("Görüşmek üzere Efendim Kaan.")
            break
        else:
            elma_konus("Bunu henüz bilmiyorum ama isterseniz 'araştır' diyerek öğrenmemi sağlayabilirsiniz.")

if __name__ == "__main__":
    elma_baslat()
EOF

